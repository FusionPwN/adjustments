{
    "name": "vanilo/adjustments",
    "description": "Vanilo Adjustments Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["adjustments", "shipping", "promotions", "taxes", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/vanilophp/adjustments"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "konekt/concord": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "orchestra/testbench": "~4.0|~5.0|~6.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Adjustments\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Adjustments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-2.x": "2.2.x-dev"
        }
    }
}
